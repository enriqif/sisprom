	<!-- Medico -->
	<navigation-rule>
		<from-view-id>*</from-view-id>
		<navigation-case>
			<from-outcome>home</from-outcome>
			<to-view-id>/faces/views/medico/altaMedico.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>error</from-outcome>
			<to-view-id>/faces/index.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	
</faces-config>
