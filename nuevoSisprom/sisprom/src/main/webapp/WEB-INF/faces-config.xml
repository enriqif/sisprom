<?xml version="1.0" encoding="UTF-8"?>
<faces-config version="2.1" xmlns="http://java.sun.com/xml/ns/javaee"
	xmlns:xi="http://www.w3.org/2001/XInclude" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_2_1.xsd">
	
	<managed-bean>
		<managed-bean-name>masterManagedBean</managed-bean-name>
		<managed-bean-class>com.sisprom.framework.managedBean.MasterManagedBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	
	<managed-bean>
		<managed-bean-name>medicoManagedBean</managed-bean-name>
		<managed-bean-class>com.sisprom.framework.managedBean.MedicoManagedBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	
	<!-- Navigation rules -->
	<navigation-rule>
		<from-view-id>/index.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>homeMedico</from-outcome>
			<to-view-id>/views/medico/homeMedico.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>errorHome</from-outcome>
			<to-view-id>/index.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/views/medico/homeMedico.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>altaMedico</from-outcome>
			<to-view-id>/views/medico/altaMedico.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
</faces-config>
