<?xml version="1.0" encoding="UTF-8"?>
<faces-config version="2.1" xmlns="http://java.sun.com/xml/ns/javaee"
 xmlns:xi="http://www.w3.org/2001/XInclude"
 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_2_1.xsd">

 <managed-bean>
  <managed-bean-name>loginManagedBean</managed-bean-name>
  <managed-bean-class>com.sisprom.framework.managedBean.LoginManagedBean</managed-bean-class>
  <managed-bean-scope>session</managed-bean-scope>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>personalManagedBean</managed-bean-name>
  <managed-bean-class>com.sisprom.framework.managedBean.PersonalManagedBean</managed-bean-class>
  <managed-bean-scope>session</managed-bean-scope>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>masterManagedBean</managed-bean-name>
  <managed-bean-class>com.sisprom.framework.managedBean.MasterManagedBean</managed-bean-class>
  <managed-bean-scope>session</managed-bean-scope>
 </managed-bean>
 <navigation-rule>
  <from-view-id>/views/usuario/login.xhtml</from-view-id>
  <navigation-case>
   <from-outcome>bienvenido</from-outcome>
   <to-view-id>/views/usuario/bienvenido.xhtml</to-view-id>
  </navigation-case>
 </navigation-rule>
 <!-- reglas de navegacion para el menu -->
 <navigation-rule>
  <from-view-id>*</from-view-id>
  <navigation-case>
   <from-outcome>paciente</from-outcome>
   <to-view-id>/views/paciente/homePaciente.xhtml</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-outcome>medico</from-outcome>
   <to-view-id>/views/medico/homeMedico.xhtml</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-outcome>turno</from-outcome>
   <to-view-id>/views/turnos/homeTurno.xhtml</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-outcome>informe</from-outcome>
   <to-view-id>/views/informes/homeInformes.xhtml</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-outcome>personal</from-outcome>
   <to-view-id>/views/personal/homePersonal.xhtml</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-outcome>error</from-outcome>
   <to-view-id>/views/error/error.xhtml</to-view-id>
  </navigation-case>
 </navigation-rule>
 <!-- Reglas para navegacion desde el submenu izquierdo ubicado en la vista
  "/homePaciente.xhtml" -->
 <navigation-rule>
  <from-view-id>/views/paciente/*</from-view-id>
  <navigation-case>
   <from-outcome>nuevoPaciente</from-outcome>
   <to-view-id>/views/paciente/nuevoPaciente.xhtml</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-outcome>archivarPaciente</from-outcome>
   <to-view-id>/views/paciente/archivarPaciente.xhtml</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-outcome>actualizarPaciente</from-outcome>
   <to-view-id>/views/paciente/actualizarPaciente.xhtml</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-outcome>buscarPaciente</from-outcome>
   <to-view-id>/views/paciente/buscarPaciente.xhtml</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-outcome>historialPaciente</from-outcome>
   <to-view-id>/views/paciente/historialPaciente.xhtml</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-outcome>error</from-outcome>
   <to-view-id>/views/error/error.xhtml</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-outcome>confirmar</from-outcome>
   <to-view-id>/views/paciente/homePaciente.xhtml</to-view-id>
  </navigation-case>
 </navigation-rule>
 <!-- Reglas para navegacion desde el submenu izquierdo ubicado en la vista 
		"/homePersonal.xhtml" -->
 <navigation-rule>
  <from-view-id>/views/personal/*</from-view-id>
  <navigation-case>
   <from-outcome>nuevoPersonal</from-outcome>
   <to-view-id>/views/personal/nuevoPersonal.xhtml</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-outcome>confirmar</from-outcome>
   <to-view-id>/views/personal/homePersonal.xhtml</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-outcome>moficarPersonal</from-outcome>
   <to-view-id>/views/personal/modificarPersonal.xhtml</to-view-id>
   </navigation-case>
  <navigation-case>
  	<from-outcome>guardadoOk</from-outcome>
  	<to-view-id>/views/personal/guardadoCorrecto.xhtml</to-view-id>
  </navigation-case>
  <navigation-case>
	<from-outcome>errorGuardado</from-outcome>
  	<to-view-id>/views/error/error.xhtml</to-view-id>
  </navigation-case>	
 </navigation-rule>
 <navigation-rule>
  <from-view-id>/views/paciente/nuevoPaciente.xhtml</from-view-id>
  <navigation-case>
   <from-outcome>hecho</from-outcome>
   <to-view-id>/views/hecho.xhtml</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-outcome>error</from-outcome>
   <to-view-id>/views/paciente/error.xhtml</to-view-id>
  </navigation-case>
 </navigation-rule>
 <!-- Reglas para navegacion desde el submenu izquierdo ubicado en la vista
  "/homeMedico.xhtml" -->
 <navigation-rule>
  <from-view-id>/views/medico/*</from-view-id>
  <navigation-case>
   <from-outcome>buscarMedico</from-outcome>
   <to-view-id>/views/medico/buscarMedico.xhtml</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-outcome>modificarPerfil</from-outcome>
   <to-view-id>/views/medico/modificarPerfil.xhtml</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-outcome>hechoMedico</from-outcome>
   <to-view-id>/views/medico/hechoMedico.xhtml</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-outcome>error</from-outcome>
   <to-view-id>/views/error/error.xhtml</to-view-id>
  </navigation-case>
  <navigation-case>
  	<from-outcome>confirmar</from-outcome>
  	<to-view-id>/views/medico/homeMedico.xhtml</to-view-id>
  </navigation-case>
 </navigation-rule> 
</faces-config>
